    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.12",
    "@types/node-schedule": "^2.1.7",
    "@types/paytmchecksum": "^1.5.0",
    "@types/pg": "^8.11.10",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.2",
    "@types/twilio": "^3.19.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.6.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-schedule": "^2.1.1",
    "paytmchecksum": "^1.5.1",
