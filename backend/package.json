{
  "name": "ludoversebackend",
  "proxy": "http://localhost:3000",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --verbose -e js,ts,json --exec \"npm run build && node dist/server.js\"",
    "test": "jest --passWithNoTests",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d dist/config/database.js",
    "migration:run": "npm run build && typeorm-ts-node-commonjs migration:run -d dist/config/database.js",
    "migration:revert": "npm run build && npm run typeorm migration:revert -- -d dist/config/database.js",
    "migration:show": "npm run build && npm run typeorm migration:show -- -d dist/config/database.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
